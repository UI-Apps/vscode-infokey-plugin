{
    "name": "infokey",
    "displayName": "Infokey",
    "description": "Extension to add syntax highlighting and snippets for ~/.infokey config.",
    "icon": "./images/logo.png",
    "version": "1.0.0",
    "repository": "https://github.com/UI-Apps/vscode-infokey-plugin",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "infokey",
            "aliases": ["infokey"],
            "extensions": ["infokey"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "infokey",
            "scopeName": "source.infokey",
            "path": "./syntaxes/infokey.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "infokey",
                "path": "./snippets/infokey.json"
            }
        ]
    }
}